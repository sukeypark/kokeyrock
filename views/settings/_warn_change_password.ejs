<!-- Form to warn user that needs to change passwrod -->
<div id="warn_change_password" class="modal in" style="top: 0px; display: none;" aria-hidden="false">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h3><%=translation.settings.warn_change_pass.title%></h3>
            </div>
            <div class="modal-body clearfix">
                <div class="right">
                    <p><%=translation.settings.warn_change_pass.content%></p>
                </div>
            </div>
            <div class="modal-footer">
                <button data-dismiss="modal" class="btn btn-primary"><%=translation.settings.warn_change_pass.continue_btn%></button>
            </div>
        </div>
    </div>
</div>
